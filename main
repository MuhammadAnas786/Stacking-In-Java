import java.util.ArrayList;
import java.util.Scanner;
public class Driver{
public static ArrayList<City> Cities=new ArrayList<City>();

public static Scanner ScannerObject=new Scanner(System.in);

public static void main(String args[]){


City City1=new City(ScannerObject.nextLine(),ScannerObject.nextInt(),500,true);

ScannerObject.nextLine();ScannerObject.nextLine();

City City2=new City(ScannerObject.nextLine(),ScannerObject.nextInt(),700,false);


Cities.add(City1);

Cities.add(City2);

System.out.println("City with maximum population is"+MaxPopCity());

Cities.remove(0);

 }


public static City MaxPopCity(){
	int i=0;

	while(i<Cities.size())
	{
		if(Cities.get(i).getPopulation()==City.getmaxPopulation())
		break;
		
		i++;
	}
return Cities.get(i);
}

} 
